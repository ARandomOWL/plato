name:    concepts
version: 0.0.1

build-type:    Simple
cabal-version: >=1.10

executable concept-simulate
    main-is: Main.hs

    build-depends: base
                 , mtl

    default-language: Haskell2010
    GHC-options:      -Wall -fwarn-tabs

test-suite concepts-test
    type:    exitcode-stdio-1.0
    main-is: Test.hs

    build-depends: base
                 , mtl

    default-language: Haskell2010
